{
  "name": "YangTab",
  "description": "a new tab for you, help you like it!",
  "version": "0.1",
  "incognito": "split",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    }
  },
  "manifest_version": 2,
  "permissions": [
    "storage"
  ]
}